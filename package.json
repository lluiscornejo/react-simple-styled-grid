{
  "name": "react-simple-styled-grid",
  "version": "1.1.0",
  "description": "React simple styled components grid",
  "main": "./lib",
  "files": [
    "dist",
    "lib"
  ],
  "keywords": [
    "styled-components",
    "grid"
  ],
  "scripts": {
    "clean": "rimraf build public dist",
    "start": "cross-env NODE_ENV=development npm run server",
    "prepublish": "babel ./src --out-dir ./lib && npm run build-pro",
    "build-dev": "npm run clean && cross-env NODE_ENV=development webpack",
    "build-pro": "npm run clean && cross-env NODE_ENV=production webpack",
    "build": "npm run build",
    "server": "webpack-dev-server",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "test-dev": "cross-env NODE_ENV=development TEST_MODE=true jest",
    "test-pro": "cross-env NODE_ENV=production TEST_MODE=true jest",
    "test-browser": "cross-env NODE_ENV=development TEST_MODE=true jest --coverage",
    "test-debug": "cross-env NODE_ENV=development TEST_MODE=true node --inspect-brk node_modules/.bin/jest --runInBand",
    "test-watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lluiscornejo/react-simple-styled-grid"
  },
  "author": "Luis Cornejo",
  "license": "ISC",
  "peerDependencies": {
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "styled-components": "^4.2.0"
  },
  "homepage": "https://lluiscornejo.github.io/react-simple-styled-grid/index.html",
  "dependencies": {
    "browserslist": "^4.5.2",
    "caniuse-lite": "^1.0.30000951",
    "identity-obj-proxy": "^3.0.0",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "styled-components": "^4.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.3.4",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.2",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-styled-components": "^1.10.0",
    "chai": "^4.1.2",
    "chai-enzyme": "^1.0.0-beta.1",
    "copy-webpack-plugin": "^4.5.2",
    "cross-env": "^5.2.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "eslint": "^4.10.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.5.1",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^5.5.1",
    "image-webpack-loader": "^4.3.1",
    "jest": "^23.6.0",
    "jest-cli": "^23.6.0",
    "jest-enzyme": "^7.0.2",
    "jest-puppeteer": "^3.9.1",
    "jest-styled-components": "^6.3.1",
    "mocha": "^4.0.1",
    "node-sass": "^4.9.3",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "react-styleguidist": "^13.1.1",
    "react-test-renderer": "^16.8.4",
    "rimraf": "^2.6.2",
    "sinon": "^4.1.1",
    "style-loader": "^0.23.0",
    "terser-webpack-plugin": "^1.4.1",
    "url-loader": "^1.1.1",
    "webpack": "^4.19.1",
    "webpack-cli": "^3.1.1",
    "webpack-dev-server": "^3.1.8"
  }
}
